{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "prisma/*.prisma",
        "watchAssets": true
      }
    ]
  },
  "projects": {
    "api": {
      "type": "application",
      "entryFile": "main"
    },
    "seed": {
      "type": "application",
      "entryFile": "seed/seed"
    }
  }
}
